<?php
session_start();

function loadClass($class) {
	require str_replace("\\","/",strtolower($class).'.inc.php');
}

spl_autoload_register('loadClass');

?>
<!DOCTYPE HTML>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<title></title>
		<link rel="stylesheet" href="css/style.css">
	</head>
	<body>
		<h1>Hello</h1>
	</body>
</html>
